generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Availability {
  id            Int      @id @default(autoincrement())
  interviewerId String
  slots         String[]
}

model Interview {
  id            Int      @id @default(autoincrement())
  interviewerId String
  customerName  String
  slot          String
}
